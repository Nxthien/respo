<% if @course %>
  <%= react_component "CoursesShowBox",
    props: {
      program: @program,
      course: @course,
      course_subjects: @supports.course_subjects,
      remain_surveys: @supports.remain_surveys,
      selected_surveys: @supports.selected_surveys,
      remain_testings: @supports.remain_testings,
      selected_testings: @supports.selected_testings,
      members_ids: @supports.members.pluck(:id),
    }, prerender: false %>
<% else %>
  <div><%= I18n.t "courses.no_content" %></div>
<% end %>
